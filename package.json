{
  "name": "typegone",
  "version": "1.2.5",
  "description": "Convert TypeScript to plain JavaScript by stripping all types and JSDoc annotations.",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "bin": {
    "typegone": "./dist/cli.js"
  },
  "scripts": {
    "test": "npm run build && npx vitest run --reporter verbose --coverage",
    "format": "prettier --write **/*.{js,ts}",
    "format:check": "prettier --check **/*.{js,ts}",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "npx tsup"
  },
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "any",
    "typegone",
    "cli",
    "converter",
    "ts-ignore",
    "typescript-free"
  ],
  "author": "Yuki_akai",
  "homepage": "https://github.com/yukiakai212/typegone",
  "funding": "https://github.com/sponsors/yukiakai212",
  "engines": {
    "node": ">= 16"
  },
  "maintainers": [
    {
      "name": "Yuki_akai",
      "homepage": "https://github.com/yukiakai212"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/typegone.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/typegone/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.30.1",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "pkg-sandbox": "^1.0.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@yukiakai/find-up": "^1.1.6",
    "esbuild": "^0.25.8",
    "fast-glob": "^3.3.3",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "ts-morph": "^26.0.0",
    "vanipath": "^1.0.8"
  }
}
